
// declaring sets
let setOne: Set<Int>
let setTwo = Set<Int>()

let someArray = [1,2,3,1]
let someSet:Set<Int> = [1,2,3,1] // using array literals to create a set
let anotherSet:Set = [1,2,3,1]

// accessing and properties
print(someSet.isEmpty)
print(someSet.count)

print(someSet.contains(1))
print(someSet.first)

// adding and removing
var myTimes: Set = ["8am","9am","10am"]
myTimes.insert("11am")
myTimes.remove("8am")

// iterating through a set
for element in myTimes {
    print(element)
}
